@article{birosParallelLagrangeNewtonKrylovSchurMethods2005a,
  title = {Parallel {{Lagrange--Newton--Krylov--Schur Methods}} for {{PDE-Constrained Optimization}}. {{Part I}}: {{The Krylov--Schur Solver}}},
