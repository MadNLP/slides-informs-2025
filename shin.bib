}

@article{mullerAnalysisOptimizationCertain1972,
  title = {Analysis and Optimization of Certain Qualities of Controllability and Observability for Linear Dynamical Systems},
  author = {M{\"u}ller, P. C. and Weber, H. I.},
  year = 1972,
  month = may,
  journal = {Automatica},
