  keywords = {Mathematics - Optimization and Control},
  file = {/Users/sushin/Zotero/storage/TJX3VYA7/Pacaud et al. - 2023 - Parallel Interior-Point Solver for Block-Structure.pdf;/Users/sushin/Zotero/storage/E34PXWVK/2301.html}
